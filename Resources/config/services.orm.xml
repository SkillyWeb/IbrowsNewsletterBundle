<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ibrows_newsletter.mandant_manager" class="Ibrows\Bundle\NewsletterBundle\Service\orm\MandantManager">
			<argument type="service" id ="doctrine" />
			<argument>%ibrows_newsletter.classes.model.mandant%</argument>
			<argument>%ibrows_newsletter.classes.model.newsletter%</argument>
			<argument>%ibrows_newsletter.classes.model.subscriber%</argument>
			<argument>%ibrows_newsletter.classes.model.design%</argument>
			<argument>%ibrows_newsletter.classes.model.user%</argument>
        </service>
    </services>

</container>